{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "dialog-access",
  "description": "Allow open dialog in main window",
  "windows": ["main"],
  "webviews": ["main"],
  "permissions": [
    "dialog:allow-open"
  ],
  "local": true,
  "remote": {
    "urls": ["http://localhost:*/*"]
  }
}